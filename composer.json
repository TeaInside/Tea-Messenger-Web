{
    "name": "ammarfaizi2/icetea",
    "description": "IceTea PHP Framework",
    "keywords": [
        "framework",
        "php",
        "icetea"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ammar Faizi",
            "email": "ammarfaizi2@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.4",
        "filp/whoops": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "IceTea\\": "framework/IceTea/"
        },
        "files": [
            "framework/init/composer.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}